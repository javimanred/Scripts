USE [msdb]
GO
select 'Exec msdb.dbo.sp_update_job @job_id=N'''+cast(job_id as nvarchar(max))+''',
		@notify_level_email=2,
		@notify_level_page=2,
		@notify_email_operator_name=N''SupportOperator''' from sysjobs where notify_level_email = 0 and name not like '_Orbit%' and name not in('syspolicy_purge_history')
